config_version=5

[application]

config/name="Demir Yolcusu"
config/description="Türkiye'nin demiryollarında geçen isometrik pixel art tren yönetim oyunu"
config/version="0.1.0"
config/features=PackedStringArray("4.4", "Mobile")

[autoload]

EventBus="*res://src/events/event_bus.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/gdUnit4/plugin.cfg")

[display]

window/size/viewport_width=540
window/size/viewport_height=960
window/size/window_width_override=540
window/size/window_height_override=960
window/handheld/orientation=1
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_width"

[gui]

theme/default_font_size=12

[input]

touch_tap={
"deadzone": 0.5,
"events": [Object(InputEventScreenTouch,"resource_local_to_scene":false,"resource_name":"","device":-1,"index":0,"position":Vector2(0, 0),"canceled":false,"pressed":true,"double_tap":false)]
}

[internationalization]

locale/fallback="tr"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="mobile"
